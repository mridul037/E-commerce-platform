{
  "name": "dailyobjects-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged",
    "remove:dist": "rm -rf dist",
    "build:client:prod": "npm run ng-high-memory -- build --configuration production --optimization=true",
    "build:client:stage": "npm run ng-high-memory -- build --configuration staging",
    "build:client:dev": "npm run ng-high-memory -- build --configuration dev",
    "build:ssr:prod": "ng run dailyobjects-web-app:server:production && npm run compile:server",
    "build:ssr:stage": "ng run dailyobjects-web-app:server:staging && npm run compile:server",
    "build:ssr:dev": "ng run dailyobjects-web-app:server:dev && npm run compile:server",
    "build:prod": "npm run remove:dist && npm run build:client-and-server-bundles:prod && npm run compile:server && cp bin/ecosystem.config.js dist/ecosystem.config.js",
    "build:stage": "npm run remove:dist && npm run build:client-and-server-bundles:staging && npm run compile:server && cp bin/ecosystem.config.js dist/ecosystem.config.js",
    "build:dev": "npm run remove:dist && npm run build:client-and-server-bundles:dev && npm run compile:server && cp bin/ecosystem.config.js dist/ecosystem.config.js",
    "serve:client:prod": "ng serve --configuration production",
    "serve:client:stage": "ng serve --configuration staging",
    "serve:client:dev": "ng serve --configuration dev",
    "serve:client:hmr": "ng serve --configuration hmr -- --hmr",
    "serve:ssr": "cd dist && node server.js",
    "build:client-and-server-bundles:prod": "npm run build:client:prod && npm run ng-high-memory run dailyobjects-web-app:server:production",
    "build:client-and-server-bundles:staging": "npm run build:client:stage && npm run ng-high-memory run dailyobjects-web-app:server:staging",
    "build:client-and-server-bundles:dev": "npm run build:client:dev && npm run ng-high-memory run dailyobjects-web-app:server:dev",
    "compile:server": "webpack --config webpack.server.config.js",
    "universal": "npm run build:ssr:prod && npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
		"@angular/http": "^7.2.0",
		"@angular/material": "^7.2.0",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/platform-server": "^7.2.0",
		"@angular/router": "~7.2.0",
		"@ngrx/effects": "^7.1.0",
		"@ngrx/entity": "^7.1.0",
		"@ngrx/router-store": "^7.1.0",
		"@ngrx/store": "^7.1.0",
		"@ngrx/store-devtools": "^7.1.0",
		"@nguniversal/express-engine": "^7.1.0",
		"@nguniversal/module-map-ngfactory-loader": "^7.1.0",
		"@ngx-lite/json-ld": "^0.6.2",
		"@ngx-toolkit/device": "^8.0.0",
		"angularfire2": "^5.2.1",
		"core-js": "^2.6.1",
		"express": "^4.15.2",
		"firebase": "^6.3.5",
		"hammerjs": "^2.0.8",
		"ngx-color-picker": "^7.4.0",
		"ngx-cookie-service": "^2.2.0",
		"ngx-device-detector": "^1.3.5",
		"ngx-snowf": "0.0.1",
		"node-fetch": "^2.6.0",
		"rollbar": "^2.13.0",
		"rxjs": "~6.3.3",
		"supports-webp": "^2.0.1",
		"ssr-window": "^1.0.1",
		"swiper": "^5.2.0",
		"tslib": "^1.9.3",
		"xmlhttprequest": "^1.8.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^7.2.0",
		"@angular-builders/dev-server": "^7.2.1",
		"@angular-devkit/build-angular": "^0.12.1",
		"@angular/cli": "~7.2.1",
		"@angular/compiler-cli": "^7.2.0",
		"@angular/language-service": "~7.2.0",
		"@angularclass/hmr": "^2.1.3",
		"@ngrx/schematics": "^7.1.0",
		"@types/express": "^4.16.0",
		"@types/jasmine": "~3.3.5",
		"@types/jasminewd2": "~2.0.6",
		"@types/node": "^10.12.19",
		"@types/webpack-env": "^1.13.6",
		"browser-sync": "^2.26.7",
		"codelyzer": "~4.5.0",
		"husky": "^1.3.1",
		"jasmine-core": "~3.3.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~3.1.4",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.4",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"ng-packagr": "^5.3.0",
		"prettier": "1.16.1",
		"pretty-quick": "^1.10.0",
		"protractor": "~5.4.2",
		"rxjs-tslint": "^0.1.6",
		"ts-loader": "^5.3.3",
		"ts-node": "~7.0.1",
		"tsickle": ">=0.34.0",
		"tslib": "^1.9.0",
		"tslint": "~5.12.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-sonarts": "^1.8.0",
		"typescript": "~3.2.2",
		"webpack-cli": "^3.2.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format:fix && npm run lint"
		}
	}
}
